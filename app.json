{
  "expo": {
    "name": "UIUC Courses",
    "slug": "uiuc-course-explorer",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.uiuc-course-explorer",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.297866081065-o9bpl6ekbb3m2bbpiji678eck2renio7"
        }
      }
    },
    "android": {
      "package": "com.example.uiuc_course_explorer",
      "googleServicesFile": "./google-services.json"
    }
  }
}
